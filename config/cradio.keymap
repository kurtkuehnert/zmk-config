#define RUM 17
#define RDM 27
#define RUR 18
#define RDR 28
#define RUP 19
#define RDP 29
#define RUT 32
#define RDT 33
#define RBI 16 26
#define RBM 17 27
#define RBR 18 28
#define RBP 19 29
#define RBT 32 33

#define MERGE_(a,b)  a##b
#define LABEL_(a) MERGE_(combo_, a)
#define UNIQUE_COMBO LABEL_(__LINE__)

#define LAYER_COMBO(KEYPOS)          \
    UNIQUE_COMBO {                   \
        layers = <L_TAI>;            \
        timeout-ms = <50>;           \
        key-positions = <KEYPOS>;    \
        bindings = <&lm L_TAI2 SPC>; \
    };

#define COMBO1(KEYPOS, KEY)        \
    UNIQUE_COMBO {                 \
        layers = <L_TAI>;          \
        timeout-ms = <50>;         \
        key-positions = <KEYPOS>;  \
        bindings = <&kp KEY>;      \
    };

#define COMBO2(KEYPOS, KEY)        \
    UNIQUE_COMBO {                 \
        layers = <L_TAI2>;         \
        timeout-ms = <50>;         \
        key-positions = <KEYPOS>;  \
        bindings = <&kp KEY>;      \
    };

