#define LDI 23
#define LUM 12
#define LDM 22
#define LUR 11
#define LDR 21
#define LUP 10
#define LDP 20
#define LUT 31
#define LDT 30
#define LBI 13 23
#define LBM 12 22
#define LBR 11 21
#define LBP 10 20
#define LBT 31 30

#define RUI 16
